    "mkdirp": "^0.5.1",
    "ng-router-loader": "^2.1.0",
    "ngc-webpack": "^1.1.3",
    "node-sass": "^4.4.0",
    "node-static": "^0.7.8",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-loader": "^1.2.2",
    "protractor": "4.0.14",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.8.4",
    "resolve-url-loader": "^1.6.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^2.0.0",
    "ts-node": "^2.0.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.2.0",
    "typescript": "2.0.10",
    "url-loader": "^0.5.7",
    "webdriver-manager": "^11.1.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^2.6.0"
  },
  "greenkeeper": {
    "ignore": [
      "typescript",
      "codelyzer",
      "@types/core-js",
      "@types/jasmine",
      "@types/jquery",
      "@types/lodash",
      "@types/node",
      "@types/selenium-webdriver"
    ]
  }
}
