{
  "name": "Angular-webpack2-skeleton",
  "version": "1.0.0-alpha.10",
  "author": "Stefano Cappa",
  "description": "Angular skeleton with webpack2",
  "license": "Apache-v2",
  "main": "src/main.ts",
  "homepage": "https://www.stefanocappa.it",
  "private": true,
  "repository": "https://github.com/Ks89/Angular2-webpack2-skeleton",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.8.6"
  },
  "scripts": {
    "start": "webpack-dev-server --progress --port 8080",
    "prebuild": "npm run clean-build",
    "build:dev": "webpack --config config/webpack.dev.js --colors --progress --display-error-details --display-cached --profile",
    "build:prod": "webpack --config config/webpack.prod.js --colors --progress --display-error-details --display-cached --profile",
    "build:prod:aot": "npm run clean-awcache && npm run clean-aot && webpack --config config/webpack.prod.js --colors --progress --display-error-details --display-cached --profile",
    "clean": "npm run clean-build && npm run clean-coverage && npm run clean-aot && npm run clean-awcache",
    "clean-build": "rimraf dist",
    "clean-coverage": "rimraf coverage",
    "clean-aot": "rimraf aot",
    "clean-awcache": "rimraf .awcache",
    "tslint": "tslint",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "pretest": "npm run clean-coverage",
    "test": "karma start --single-run",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run e2e:only",
    "e2e:only": "npm run protractor",
    "protractor": "protractor",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "coveralls": "coveralls < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info",
    "next-update": "next-update -k true --tldr"
  },
  "dependencies": {
    "@angular/common": "^2.4.7",
    "@angular/compiler": "^2.4.7",
    "@angular/compiler-cli": "^2.4.7",
    "@angular/core": "^2.4.7",
    "@angular/forms": "^2.4.7",
    "@angular/http": "^2.4.7",
    "@angular/platform-browser": "^2.4.7",
    "@angular/platform-browser-dynamic": "^2.4.7",
    "@angular/platform-server": "^2.4.7",
    "@angular/router": "^3.4.7",
    "@angularclass/idle-preload": "^1.0.4",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.20",
    "bootstrap": "4.0.0-alpha.5",
    "bootstrap-loader": "2.0.0-beta.20",
    "core-js": "^2.4.1",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "ng2-validators": "^1.7.0",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.1.0",
    "tether": "^1.3.8",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angularclass/hmr": "^1.2.2",
    "@angularclass/hmr-loader": "^3.0.2",
    "@types/core-js": "^0.9.34",
    "@types/jasmine": "^2.5.41",
    "@types/jquery": "^2.0.40",
    "@types/lodash": "4.14.50",
    "@types/node": "^7.0.5",
    "@types/selenium-webdriver": "^2.53.39",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^6.7.2",
    "awesome-typescript-loader": "^3.0.3",
    "browser-sync": "^2.18.7",
    "browser-sync-webpack-plugin": "^1.1.4",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "codelyzer": "^2.0.0",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^1.2.0",
    "coveralls": "^2.11.16",
    "css-loader": "^0.26.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "font-awesome-sass-loader": "^1.0.3",
    "fontawesome": "^4.7.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-edge-launcher": "^0.2.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-diff-reporter": "^1.0.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "^0.1.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "mkdirp": "^0.5.1",
    "ng-router-loader": "^2.1.0",
    "ngc-webpack": "^1.2.0",
    "node-sass": "^4.5.0",
    "node-static": "^0.7.8",
    "null-loader": "^0.1.1",
    "optimize-js-plugin": "0.0.4",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-loader": "^1.2.2",
    "protractor": "^5.1.1",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.9.1",
    "resolve-url-loader": "^1.6.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^5.0.1",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^2.0.0",
    "ts-node": "^2.1.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.2.0",
    "typescript": "^2.1.6",
    "url-loader": "^0.5.7",
    "webdriver-manager": "^12.0.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^2.6.1"
  },
  "greenkeeper": {
    "ignore": [
      "typescript",
      "codelyzer",
      "@types/core-js",
      "@types/jasmine",
      "@types/jquery",
      "@types/lodash",
      "@types/node",
      "@types/selenium-webdriver"
    ]
  }
}
