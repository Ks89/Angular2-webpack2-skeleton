{
  "name": "angular-webpack-skeleton",
  "version": "1.0.0-beta.11",
  "author": "Stefano Cappa",
  "description": "Angular skeleton with webpack",
  "license": "MIT",
  "main": "src/main.ts",
  "homepage": "https://github.com/Ks89",
  "private": true,
  "repository": "https://github.com/Ks89/angular-webpack-skeleton",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "server:dev:hmr": "npm run server:dev -- --hotOnly",
    "server:aot:dev": "cross-env BUILD_AOT=1 npm run server:dev",
    "server:dev": "npm run webpack-dev-server -- --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist -c-1 --cors",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
    "server": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start": "npm run server:dev",
    "start:aot": "npm run server:aot:dev",
    "prebuild": "npm run clean:dist",
    "build:dev": "npm run webpack -- --config config/webpack.dev.js --progress --profile",
    "build:dev:aot": "cross-env BUILD_AOT=1 npm run build:dev",
    "build:prod": "webpack --config config/webpack.prod.js --colors --progress --display-error-details --display-cached --profile",
    "build:prod:aot": "npm run clean:aot && cross-env BUILD_AOT=1 npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
    "build:aot": "npm run build:prod:aot",
    "build": "npm run build:dev",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:aot:dev": "npm run build:dev:aot -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:aot:prod": "npm run build:prod:aot -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "node": "node",
    "clean": "npm cache clean --force && npm run clean:dist && npm run clean:dll && npm run clean:aot && npm run clean:coverage && npm run clean:docs && npm run clean:dll",
    "clean:dist": "rimraf dist",
    "clean:aot": "rimraf compiled",
    "clean:coverage": "rimraf coverage",
    "clean:docs": "rimraf docs && rimraf documentation",
    "clean:dll": "rimraf dll",
    "tslint": "tslint",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "pretest": "npm run clean:coverage",
    "test": "npm run lint && karma start --single-run",
    "test:watch": "npm run lint && karma start",
    "pree2e": "npm run build:prod:aot && npm run webdriver:update",
    "e2e": "npm-run-all -r -p server:prod protractor",
    "e2e:ci": "npm run webdriver:update && npm-run-all -r -p server:prod:ci protractor:delay",
    "e2e:live": "npm-run-all -p -r server:prod:ci protractor:live",
    "protractor": "protractor",
    "protractor:delay": "sleep 3 && npm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "webdriver:update": "webdriver-manager update --gecko false --standalone false",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "docs": "typedoc --out docs src",
    "docs:compodoc": "compodoc -p tsconfig.json",
    "docs:compodoc:serve": "compodoc -p tsconfig.json -s",
    "docs:compodoc:serve:watch": "compodoc -p tsconfig.json -s -w",
    "analyze:bundle": "webpack-bundle-analyzer dist/webpack-bundle-analyzer-report.json dist/",
    "sonar-scanner": "sonar-scanner",
    "coveralls": "coveralls < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "dependencies": {
    "@angular/animations": "^5.0.2",
    "@angular/common": "^5.0.2",
    "@angular/compiler": "^5.0.2",
    "@angular/core": "^5.0.2",
    "@angular/forms": "^5.0.2",
    "@angular/http": "^5.0.2",
    "@angular/platform-browser": "^5.0.2",
    "@angular/platform-browser-dynamic": "^5.0.2",
    "@angular/platform-server": "^5.0.2",
    "@angular/router": "^5.0.2",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.5",
    "@ngrx/store": "^4.1.0",
    "bootstrap": "^4.0.0-beta.2",
    "bootstrap-loader": "^2.2.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "mousetrap": "^1.6.0",
    "ng2-validators": "^2.2.0",
    "popper.js": "^1.12.6",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "tether": "^1.3.8",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.0.32",
    "@angular/compiler-cli": "^5.0.2",
    "@compodoc/compodoc": "^1.0.1",
    "@ngrx/store-devtools": "^4.0.0",
    "@ngtools/webpack": "^1.8.0",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.6.2",
    "@types/jquery": "^3.2.15",
    "@types/mousetrap": "^1.5.34",
    "@types/node": "^8.0.47",
    "@types/selenium-webdriver": "^3.0.8",
    "@types/source-map": "^0.5.2",
    "@types/uglify-js": "^2.6.29",
    "@types/webpack": "^3.0.14",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.4.0",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "codelyzer": "^3.2.2",
    "copy-webpack-plugin": "^4.2.0",
    "copyfiles": "^1.2.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "exports-loader": "^0.6.4",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "font-awesome-sass-loader": "^2.0.1",
    "fontawesome": "^4.7.2",
    "html-webpack-plugin": "^2.29.0",
    "http-server": "^0.10.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-diff-reporter": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-remap-coverage": "^0.1.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sonarqube-unit-reporter": "0.0.13",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.5",
    "loglevel": "^1.5.1",
    "mkdirp": "^0.5.1",
    "ngc-webpack": "^4.0.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "null-loader": "^0.1.1",
    "offline-plugin": "^4.8.4",
    "optimize-js-plugin": "^0.0.4",
    "postcss-loader": "^2.0.8",
    "protractor": "^5.2.0",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.9.5",
    "resolve-url-loader": "^2.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.8.7",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "to-string-loader": "^1.1.5",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.9.0",
    "typescript": "~2.5.2",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "^0.6.2",
    "webdriver-manager": "^12.0.6",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-merge": "^4.1.0",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "greenkeeper": {
    "ignore": [
      "typescript",
      "codelyzer",
      "@types/core-js",
      "@types/jasmine",
      "@types/jquery",
      "@types/lodash",
      "@types/node",
      "@types/selenium-webdriver"
    ]
  }
}
